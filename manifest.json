{
  "manifest_version": 3,
  "name": "Tab Collections",
  "description": "Tab Collections",
  "version": "1.0.1",
  "icons": { "16": "logo16.png", "48": "logo48.png", "128": "logo128.png" },
  "permissions": ["storage", "tabs", "tabGroups", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ],
  "host_permissions": ["<all_urls>"],
  "action": {}
}
