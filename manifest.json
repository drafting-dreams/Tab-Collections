{
  "manifest_version": 2,
  "name": "Tab Collections",
  "description": "Tab Collections",
  "version": "1.0",
  "permissions": ["storage", "tabs"],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ],
  "browser_action": {}
}
