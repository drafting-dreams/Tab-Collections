{
  "extends": ["react-app", "prettier", "prettier/react"],
  "plugins": ["prettier"],
  "parser": "babel-eslint",
  "rules": {
    "prettier/prettier": "error",
    "default-case": "off",
    "react-hooks/exhaustive-deps": "off",
    "no-restricted-globals": ["off", { "name": "self" }]
  },
  "env": {
    "browser": true
  },
  "globals": {
    "chrome": "readonly"
  }
}
