{
  "extends": ["react-app", "prettier", "prettier/react"],
  "plugins": ["prettier"],
  "parser": "babel-eslint",
  "rules": {
    "prettier/prettier": "error",
    "default-case": "off",
    "react-hooks/exhaustive-deps": "off"
  },
  "env": {
    "browser": true
  },
  "globals": {
    "chrome": "readonly"
  }
}
